              "body": {
                "providerName": "google",
                "providerToken": "4712948234y12783y",
                "email": "prolific@prolificinteractive.com"
              }
            }
          },
        ]
      },
      {
        "name": "AuthenticationApi",
        "endpoints": [
          {
            "responseModelName": "AuthenticationToken",
            "endpoint": "users/register",
            "authorized": false,
            "method": "post",
            "parameters": {
              "modelName": "AuthenticationInformation",
              "body": {
                "email": "string",
                "password": "string"
              }
            }
          }
        ]
      }
    ]
  }
