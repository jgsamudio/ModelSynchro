{
  "authorName": "Jonathan Samudio",
  "companyName": "Prolific Interactive",
  "projectName": "ModelSynchro",
  "language": "kotlin",
  "verbose": true,
  "serverAPIInfo": {
      "outputDirectory": "OpenAPIModels/",
      "outputPackage": "com.kapsch.android.base",
      "baseUrl": "https://virtserver.swaggerhub.com/Kapsch_TrafficCom/CBO_Proposal/1.0.0/",
      "endpoints": [
          {
              "name": "AuthenticationToken",
              "endpoint": "users/register",
              "authorized": false,
              "method": "post",
              "parameters": {
                  "email": "sampleEmail.com",
                  "password": "samplePassword"
              }
          },
          {
              "name": "AuthenticationToken",
              "endpoint": "users/login",
              "authorized": false,
              "method": "post",
              "parameters": {
                  "email": "sampleEmail.com",
                  "password": "samplePassword"
              }
          },
          {
              "name": "Account",
              "endpoint": "accounts",
              "authorized": false,
              "method": "get"
          },
          {
            "name": "ProviderAuthenticationInformation",
            "endpoint": "users/provider_login",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "User",
            "endpoint": "users/123456789",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "AuthenticationToken",
            "endpoint": "users/token",
            "authorized": false,
            "method": "post",
            "parameters": {
                "refreshToken": "string"
            }
          },
          {
            "name": "AppConfiguration",
            "endpoint": "users/configurations",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "PostalAvailablility",
            "endpoint": "users/availability",
            "authorized": false,
            "method": "get",
            "queries": {
                "postalCode": "11375"
            }
          },
          {
            "name": "Account",
            "endpoint": "accounts/2492349823817",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "PaymentMethods",
            "endpoint": "accounts/2492349823817/payments",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Transponders",
            "endpoint": "accounts/2492349823817/transponders",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Vehicles",
            "endpoint": "accounts/2492349823817/vehicles",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Make",
            "endpoint": "vehicles/makes",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Model",
            "endpoint": "vehicles/models",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Color",
            "endpoint": "vehicles/colors",
            "authorized": false,
            "method": "get"
          },
          {
            "name": "Vehicles",
            "endpoint": "vehicles/search",
            "authorized": false,
            "method": "post",
            "parameters": {
                "licensePlateNumber": "BROOK123",
                "state": "NY"
            }
          }
      ]
  },
  "localJSONDirectory": [
    {
      "inputDirectory": "SampleJSON1/",
      "outputDirectory": "SampleJSONOutput1/",
      "outputPackage": "com.kapsch.android.base"
    }
  ]
}
